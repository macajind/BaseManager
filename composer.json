{
	"name": "macajind/crud-manager",
	"description": "Class which brings CRUD operations to other inherited model classes using Nette Database library.",
	"homepage": "https://github.com/macajind/crud-manager",
	"authors": [
		{
			"name": "Jindřich Máca",
			"homepage": "https://github.com/macajind"
		}
	],
	"type": "library",
	"license": "BSD-3-Clause",
	"require": {
		"php": ">=7.1.0",
		"nette/database": "^2.4",
		"nette/utils": "^2.4"
	},
	"require-dev": {
		"nette/tester": "^2.0",
		"mockery/mockery": "^1.2"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	},
	"config": {
		"platform": {
			"php": "7.1"
		}
	}
}
